packages:
  yum:
    python27-setuptools: []
    dos2unix: []
container_commands:
  01-dos2unix:
      command: dos2unix -k .ebextensions/node.sh
  02-permissions:
    command: chmod 700 .ebextensions/node.sh
  03-supervise:
    command: ".ebextensions/node.sh"